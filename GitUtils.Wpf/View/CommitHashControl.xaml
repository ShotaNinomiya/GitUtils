<UserControl x:Class="GitUtils.Wpf.View.CommitHashControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GitUtils.Wpf.View"
             mc:Ignorable="d">
    <Grid>
        <TextBox Text="{Binding CommitHash}" Name="CommitHashTextBox" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0" Height="Auto"/>
    </Grid>
</UserControl>
